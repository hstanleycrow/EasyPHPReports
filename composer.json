{
    "name": "hstanleycrow/easyphpreports",
    "description": "Free PHP library For simple reports",
    "keywords": [
        "php",
        "reports",
        "excel",
        "csv",
        "pdf"
    ],
    "license": "MIT",
    "homepage": "https://github.com/hstanleycrow/EasyPHPReports",
    "authors": [
        {
            "name": "Harold Rivas AKA Harold Crow",
            "email": "holamundo@microaplicaciones.com",
            "homepage": "https://www.microaplicaciones.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "phpoffice/phpspreadsheet": "^1.29",
        "mpdf/mpdf": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "hstanleycrow\\EasyPHPReports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReportsDefinitions\\": "ReportsDefinitions/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "require-dev": {
        "vlucas/phpdotenv": "^5.6",
        "nesbot/carbon": "^2.62"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}